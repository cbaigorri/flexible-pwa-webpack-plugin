{
  "name": "flexible-pwa-webpack-plugin",
  "version": "0.0.9",
  "description": "A flexible webpack plugin for generating favicons, manifest.json and injecting HTML header tags.",
  "main": "index.js",
  "license": "MIT",
  "author": "Olivier Pascal <olivier.pascal@gmail.com>",
  "scripts": {
    "dev": "babel -w src --out-dir dist --source-maps",
    "build": "babel src --out-dir dist --copy-files",
    "clean": "rm -rf tests/*/output && rm -rf dist",
    "test": "npm run clean && npm run build && node tests/index.js",
    "prepublishOnly": "npm run build",
    "pretty": "prettier --write",
    "lint": "eslint"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "jimp": "^0.3.11",
    "mime": "^2.3.1",
    "to-ico": "^1.1.5"
  },
  "keywords": [
    "manifest",
    "manifest.json",
    "webpack",
    "icons",
    "resize",
    "favicon",
    "inject",
    "pwa",
    "progressive",
    "webapp",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/olivierpascal/flexible-pwa-webpack-plugin.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
